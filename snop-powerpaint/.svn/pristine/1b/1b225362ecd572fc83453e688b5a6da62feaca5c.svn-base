package shapes;

import java.awt.Graphics2D;

/**
 * Interface for a shape which knows how to draw itself onto a Graphics2D for PowerPaint.
 * 
 * @author Sophie Nop
 * @version 11 November 2015
 */
public interface DrawableShape {
    
    /**
     * Draw this shape onto a given Graphics2D object.
     * @param theGrapics that is drawn. 
     */
    void drawSelf(Graphics2D theGrapics);
    
}
